.music .play{
    background:url(../images/music.png) no-repeat;
    background-size:cover;
    position:fixed;
    z-index:120;
}

.music .play.playing{
    animation:1s linear infinite musicPlaying;
    -webkit-animation:1s linear infinite musicPlaying;
}

@keyframes musicPlaying{
    0%{ transform:rotate(0deg);}
    100%{ transform:rotate(360deg);}
}
