.slider2{
    background: url("../images/bg.png") no-repeat top center;
    background-size: 100% 100%;
    position: relative;
}

.hide{display: none;}
.tou{opacity: 0;}

/* 页面2css */
.slider2 .pao1{width:3%;height: auto;left:71%;top:21%;position: absolute;}
.slider2 .pao2{width:5%;height: auto;left: 73%;top:18%;position: absolute;}
.slider2 .pao3{width:31% ;height: auto;left: 68%;top:1%;position: absolute;z-index: 10;}
.slider2 .baba{width:12%;height: auto;left:77%;top:8%;;position: absolute;z-index: 12;}
.slider2 .mama{width:14%;height: auto;left:77%;top:8%;position: absolute;z-index: 12;}
.slider2 .yao1{width:38%;height: auto;left:13%;top:31.5%;position: absolute;}
.slider2 .yao2{width:92% ;height: auto;left:4%;top:15%;position: absolute;}
.slider2 .yao3{width:28%;height: auto;left:65%;top:11%;position: absolute;}
.slider2 #shoushi{width:10%;height: auto;left:67%;top:53%;position: absolute;}
.slider2 #yuan{width:8%;height: auto;left:64%;top:51%;position: absolute;-webkit-animation: mysuo infinite linear 1.5s;opacity: 1;}
.slider2 #pai1{width:70%;height: auto;left:14%;top:17%;position: absolute;-webkit-transform: rotate(-17deg);}
.slider2 #pai2{width:32%;height: auto;left: 1%;top:18%;position: absolute;}

.slider2 .yao{-webkit-perspective:600;-webkit-transform:perspective(600px) rotate(0deg);-webkit-animation: myyao 2 linear 3s;-webkit-transform-origin:center;}
@-webkit-keyframes myyao {
    0%{-webkit-transform: rotate(0deg)}
    25%{-webkit-transform: rotate(3deg)}
    75%{-webkit-transform: rotate(-3deg)}
    100%{-webkit-transform: rotate(0deg)}
}
.slider2 .down{ display:block;-webkit-animation: mydown  1 forwards ease-out 2s; opacity: 0;}
@-webkit-keyframes mydown {
    0%{opacity: 0;}
    100%{opacity: 1;}
}
.slider2 .ding{display:block; opacity: 1;-webkit-transform:scale(1.6,1.6);-webkit-animation: myding 1 forwards linear .5s;}
@-webkit-keyframes myding {
    0%{ -webkit-transform:scale(1.6,1.6);}
    100%{-webkit-transform:scale(1,1);}
}
@-webkit-keyframes mysuo {
    0%{opacity: 1;}
    50%{opacity: 0;}
    100%{opacity: 1 ;}
  }
